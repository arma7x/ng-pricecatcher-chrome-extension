{
  "name": "PriceCatcher - OpenDOSM",
  "version": "1.0",
  "description": "PriceCatcher app for OpenDOSM",
  "manifest_version": 3,
  "permissions": [
      "tabs",
      "activeTab",
      "webNavigation",
      "storage",
      "scripting"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
      "default_popup": "index.html"
  },
  "host_permissions": ["https://github.com/"]
}
